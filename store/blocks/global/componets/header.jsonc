
{
  "header-layout.desktop": {
    "children": ["sticky-layout#desktop"]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": ["flex-layout.row#desktop", "flex-layout.row#menu-bar"]
  },

  //children 1
  "flex-layout.row#desktop": {
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "flex-layout.col#spacer",
      "search-bar",
      "locale-switcher",
      "minicart.v2",
      "login"
    ]
  },
  "flex-layout.col#logo-desktop": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["logo"]
  },
  "logo": {
    "props": {
      "title": "Logo",
      "href": "/",
      
      "url": "assets/img/Icon_Black.svg",
      // "url": "assets/img/Icon_Black.svg",
      "width": "40"
    }
  },
  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },

  //children 2
  "flex-layout.row#menu-bar": {
    "props": {
      // "horizontalAlign": "center",
      // "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "menu-bar",
      "topMenuIcons": true
      

    },
    "children": [
      "flex-layout.col#menu-food",
      "flex-layout.col#menu-fitness",
      "flex-layout.col#menu-artico",
      "flex-layout.col#menu-Poolday",
      "flex-layout.col#menu-hoteles",
      "flex-layout.col#menu-casas",
      "flex-layout.col#menu-naturales",
      "flex-layout.col#menu-closet",
      "flex-layout.col#menu-playas",
      "flex-layout.col#menu-alpinismo",
      "flex-layout.col#menu-delivery",
      "flex-layout.col#menu-cerveza"
    ]
  },

