  "flex-layout.col#menu-food": {
    "title": "food container",
    "props": {
      "horizontalAlign": "center",
      "verticalAlign": "center"
    },
    "children": ["icon#menu-food", "rich-text#menu-food"]
  },
  "icon#menu-food": {
    "props": {
      "id": "food",
      "href": "/",
      "url": "arquivos/fastfood.svg",
      // "url": "/assets/img/hotel.svg",
     "blockClass": "menu-food",
      "style": {
        "display": "inline-block"
      }
    }
  },
  "rich-text#menu-food": {
    "title": "text food",
    "props": {
      "text": "Food",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-6",
      "blockClass": ["menu-header"]
    }
  },

