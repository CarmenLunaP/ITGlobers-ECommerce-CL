    "children": [
      "drawer",
      "flex-layout.col#logo-desktop",
      "search-bar",
      "flex-layout.col#spacer",
      "menu-item#trendsITG",
      "menu-item#collection",
      "menu-item#deals",
      "menu-item#cyber-mondays",
      "menu-item#never-say-never",
      // "locale-switcher",
      "minicart.v2",
      "login"
    ]
  },

"search-bar": {
  "props": {
    "placeholder": "Search products....",
    "blockClass": "search-bar-home-desktop"
